# Group Assignment 1

samples <- c(32.88, 40.63, 34.43, 39.97, 36.9, 34.71, 57.74, 57.42, 61.3, 56.47, 57.87, 59.39)
samples
mean(samples)

median(samples)

sd(samples)

geometric.mean(samples)

harmonic.mean(samples)

cv(samples)

B <- function(A){sd(A) / sqrt(length(A))}
B(samples)  
  
